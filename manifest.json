{
  "short_name": "page element blacklist",
  "name": "Page Element Blacklist",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab"],
  "background": {
    "service_worker": "public/js/background.bundle.js"
  },
  "options_page": "public/background.html",
  "action": {
    "default_popup": "public/popup.html",
    "default_icon": {
        "16": "/public/images/icon-16x16.png",
        "32": "/public/images/icon-32x32.png",
        "48": "/public/images/icon-48x48.png",
        "128": "/public/images/icon-128x128.png"
    }
}
}
